{
    "routes": [
        {
            "src": "/portfolio/:id",
            "dest": "/index.html"
        },
        {
            "src": "/error",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true
}